{
  "$ref": "#/definitions/BitcoinSegwitTransactionSignRequest",
  "$schema": "http://json-schema.org/draft-07/schema#",
  "definitions": {
    "BitcoinSegwitTransactionSignRequest": {
      "additionalProperties": false,
      "properties": {
        "callbackURL": {
          "type": "string"
        },
        "publicKey": {
          "type": "string"
        },
        "transaction": {
          "$ref": "#/definitions/BitcoinSegwitUnsignedTransaction"
        }
      },
      "required": ["publicKey", "transaction"],
      "type": "object"
    },
    "BitcoinSegwitUnsignedTransaction": {
      "additionalProperties": false,
      "properties": {
        "psbt": {
          "type": "string"
        },
        "type": {
          "enum": ["unsigned"],
          "type": "string"
        }
      },
      "required": ["psbt", "type"],
      "type": "object"
    }
  }
}
