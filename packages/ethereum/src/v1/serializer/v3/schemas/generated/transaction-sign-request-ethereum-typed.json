{
  "$schema": "http://json-schema.org/draft-07/schema#",
  "definitions": {
    "EthereumTypedTransactionSignRequest": {
      "additionalProperties": false,
      "properties": {
        "callbackURL": {
          "type": "string"
        },
        "publicKey": {
          "type": "string"
        },
        "transaction": {
          "$ref": "#/definitions/SerializableEthereumTypedUnsignedTransaction"
        }
      },
      "required": ["publicKey", "transaction"],
      "type": "object"
    },
    "HexString": {
      "type": "string"
    },
    "SerializableEthereumTypedUnsignedTransaction": {
      "additionalProperties": false,
      "properties": {
        "derivationPath": {
          "type": "string"
        },
        "masterFingerprint": {
          "type": "string"
        },
        "serialized": {
          "$ref": "#/definitions/HexString",
          "type": "string"
        },
        "type": {
          "enum": ["unsigned"],
          "type": "string"
        }
      },
      "required": ["derivationPath", "masterFingerprint", "serialized", "type"],
      "type": "object"
    }
  }
}
